package basic;

import java.util.Scanner;
public class Tests{
    public static int Sum(int a, int b) {
        return a + b;
	}
    public static void main(String[] args){
       Scanner scan = new Scanner(System.in);
       int a = scan.nextInt();
       scan.nextLine();
       int b = scan.nextInt();
       System.out.println(Sum(a, b));
    }
}
